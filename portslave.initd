#!/bin/sh
#
# radiusd	Start Radinit
#
# chkconfig: 345 98 10
#
# description:	Radinit
#

# Source function library.
. /etc/rc.d/init.d/functions

case "$1" in
  start)
	show Starting Radinit
	daemon radinit
	;;
  stop)
	;;
  restart)
	$0 stop
	$0 start
	;;
  *)
        echo "Usage: $0 {start|stop|restart}"
        exit 1
esac

exit 0
